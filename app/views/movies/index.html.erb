<h1>movies#index</h1>

<table>
  <tr>
    <th>Rank</th>
    <th>Title</th>
  </tr>
  <% @movies.each do |movie| %>
    <tr>
      <td><%= movie.rank %></td>
      <td><%= link_to "#{movie.title}", movie_path(movie.id) %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to 'Add a Movie', movies_new_path %></p>