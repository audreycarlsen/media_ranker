<table>
  <tr>
    <td>
      <h1>Top Movies</h1>

      <% @top_five_movies.each do |movie| %>
        <p><%= link_to "#{movie.title}", movie_path(movie.id) %><br>
        Ranked: <%= movie.rank %></p>
      <% end %>

      <%= link_to "View More Movies", movies_path %>
    </td>

    <td>
      <h1>Top Books</h1>

      <% @top_five_books.each do |book| %>
        <p><%= link_to "#{book.title}", book_path(book.id) %><br>
        Ranked: <%= book.rank %></p>
      <% end %>

      <%= link_to "View More Books", books_path %>
    </td>
  </tr>
</table>